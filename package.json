{
	"name": "express",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "nodemon app.js"
	},
	"paths": {
		"src": {
			"styles": "./src/styles/",
			"stylesDist": "./src/styles/dist/",
			"fontello": "./src/fonts/fontello/",
			"js": "./src/js/",
			"img": "./src/fonst/images"
		},
		"build": {
			"css": "./public/assets/css",
			"img": "./public/assets/images",
			"js": "./public/assets/js",
			"fonts": "./public/assets/fonts/",
			"fontello": "./public/assets/fonts/fontello/"
		}
	},
	"vars": {
		"siteJsName": "app.js",
		"siteCssName": "app.css",
		"scssName": "main.scss"
	},
	"globs": {
		"distCss": [
			"./src/styles/libs/*.css",
			"./src/styles/dist/main.css"
		],
		"distJs": [
			"./src/js/main.js",
			"./src/js/libs/*.js"
		],
		"img": [
			"./public/images/"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.11.1",
		"@babel/preset-env": "^7.11.0",
		"babel-preset-es2015": "^6.24.1",
		"fancy-log": "^1.3.3",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^3.1.0",
		"gulp-babel": "^8.0.0",
		"gulp-cached": "^1.1.1",
		"gulp-concat": "^2.6.0",
		"gulp-cssnano": "2.1.3",
		"gulp-filter": "^5.0.1",
		"gulp-fontello": "^0.4.6",
		"gulp-header": "^1.8.7",
		"gulp-if": "^2.0.1",
		"gulp-imagemin": "^7.1.0",
		"gulp-livereload": "^4.0.2",
		"gulp-load-plugins": "^1.3.0",
		"gulp-plumber": "^1.1.0",
		"gulp-print": "^2.0.1",
		"gulp-rename": "^1.2.2",
		"gulp-rev": "^7.1.0",
		"gulp-sass": "^4.1.0",
		"gulp-sass-glob-import": "^0.1.0",
		"gulp-size": "^2.1.0",
		"gulp-sourcemaps": "^2.2.1",
		"gulp-uglify": "^1.5.4",
		"moment": "^2.27.0",
		"nodemon": "^2.0.4"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"body-parser": "^1.19.0",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"morgan": "~1.9.1",
		"querystring": "^0.2.0",
		"twing": "^5.0.2"
	}
}
